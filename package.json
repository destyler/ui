{
  "name": "@destyler-ui/monorepo",
  "version": "0.0.1-beta.16",
  "private": "true",
  "packageManager": "pnpm@10.28.0",
  "description": "",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run --filter \"./packages/**\" -r build",
    "docs:dev": "pnpm run --filter @destyler-ui/docs -r dev",
    "docs:build": "pnpm run --filter @destyler-ui/docs -r build",
    "docs:preview": "pnpm run --filter @destyler-ui/docs -r preview",
    "docs:generate:types": "npx tsx docs/scripts/generate-type-docs.ts vue && npx tsx docs/scripts/generate-type-docs.ts react",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "release": "bumpp package.json packages/**/package.json --commit --push --tag",
    "publish:ci": "pnpm -r publish --access public --no-git-checks",
    "test": "vitest run --browser.headless",
    "test:ci": "vitest run --browser.headless",
    "test:ui": "vitest --ui --open",
    "dep": "taze",
    "vue": "pnpm --filter @destyler-ui/vue",
    "react": "pnpm --filter @destyler-ui/react",
    "generate:vue": "plop vue",
    "generate:react": "plop react"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@chromatic-com/storybook": "catalog:stories",
    "@destyler/anatomy": "catalog:prod",
    "@eslint-react/eslint-plugin": "catalog:lint",
    "@storybook/addon-a11y": "catalog:stories",
    "@storybook/addon-docs": "catalog:stories",
    "@storybook/addon-vitest": "catalog:stories",
    "@storybook/react-vite": "catalog:stories",
    "@storybook/vue3-vite": "catalog:stories",
    "@types/node": "catalog:types",
    "@vitejs/devtools": "catalog:runtime",
    "@vitest/browser-playwright": "catalog:test",
    "@vitest/coverage-v8": "catalog:test",
    "@vitest/ui": "catalog:test",
    "bumpp": "catalog:node",
    "eslint": "catalog:lint",
    "eslint-plugin-astro": "catalog:lint",
    "eslint-plugin-react-hooks": "catalog:lint",
    "eslint-plugin-react-refresh": "catalog:lint",
    "eslint-plugin-storybook": "catalog:lint",
    "happy-dom": "catalog:test",
    "playwright": "catalog:test",
    "plop": "catalog:node",
    "storybook": "catalog:stories",
    "taze": "catalog:node",
    "tsdown": "catalog:build",
    "tslib": "catalog:types",
    "typescript": "catalog:types",
    "vite": "catalog:runtime",
    "vitest": "catalog:test",
    "vitest-browser-react": "catalog:test",
    "vitest-browser-vue": "catalog:test"
  }
}
