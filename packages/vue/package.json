{
  "name": "@destyler-ui/vue",
  "type": "module",
  "version": "0.0.1-beta.7",
  "description": "A starter for creating a Vue component library.",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/destyler/ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destyler/ui.git",
    "directory": "packages/vue"
  },
  "bugs": "https://github.com/destyler/ui/issues",
  "keywords": [
    "base",
    "destyler"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "debug": "tsdown --debug",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run --browser.headless",
    "test:dev": "vitest dev --browser.headless",
    "test:ui": "vitest --ui --open"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "dependencies": {
    "@destyler/anatomy": "catalog:prod",
    "@destyler/auto-resize": "catalog:prod",
    "@destyler/calendar": "catalog:prod",
    "@destyler/carousel": "catalog:prod",
    "@destyler/checkbox": "catalog:prod",
    "@destyler/clipboard": "catalog:prod",
    "@destyler/collapse": "catalog:prod",
    "@destyler/collapsible": "catalog:prod",
    "@destyler/collection": "catalog:prod",
    "@destyler/color-picker": "catalog:prod",
    "@destyler/combobox": "catalog:prod",
    "@destyler/dialog": "catalog:prod",
    "@destyler/dom": "catalog:prod",
    "@destyler/edit": "catalog:prod",
    "@destyler/focus-trap": "catalog:prod",
    "@destyler/highlight-text": "catalog:prod",
    "@destyler/i18n": "catalog:prod",
    "@destyler/image": "catalog:prod",
    "@destyler/presence": "catalog:prod",
    "@destyler/utils": "catalog:prod",
    "@destyler/vue": "catalog:prod",
    "@destyler/xstate": "catalog:prod"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "catalog:runtime",
    "vue": "catalog:framework",
    "vue-tsc": "catalog:types"
  }
